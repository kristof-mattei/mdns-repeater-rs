    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mdns-repeater-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=mdns-repeater-rs",
                    "--package=mdns-repeater-rs"
                ],
                "filter": {
                    "name": "mdns-repeater-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "DEBUG,mdns_repeater_rs=TRACE"
            },
            "internalConsoleOptions": "openOnSessionStart",
            "terminal": "console"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mdns-repeater-rs' wrong-params",
            "cargo": {
                "args": [
                    "build",
                    "--bin=mdns-repeater-rs",
                    "--package=mdns-repeater-rs"
                ],
                "filter": {
                    "name": "mdns-repeater-rs",
                    "kind": "bin"
                }
            },
            "args": ["wrong-params"],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "mdns_repeater_rs=trace"
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mdns-repeater-rs' help",
            "cargo": {
                "args": [
                    "build",
                    "--bin=mdns-repeater-rs",
                    "--package=mdns-repeater-rs"
                ],
                "filter": {
                    "name": "mdns-repeater-rs",
                    "kind": "bin"
                }
            },
            "args": ["-h"],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "mdns_repeater_rs=trace"
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'mdns-repeater-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=mdns-repeater-rs",
                    "--package=mdns-repeater-rs"
                ],
                "filter": {
                    "name": "mdns-repeater-rs",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "DEBUG,mdns_repeater_rs=TRACE"
            },
            "internalConsoleOptions": "openOnSessionStart",
            "terminal": "console"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'integration_tests'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=integration_tests",
                    "--package=rust-end-to-end-application"
                ],
                "filter": {
                    "name": "integration_tests",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1",
                "RUST_LOG": "DEBUG,mdns_repeater_rs=TRACE"
            },
            "internalConsoleOptions": "openOnSessionStart",
            "terminal": "console"
        }
    ]
}
